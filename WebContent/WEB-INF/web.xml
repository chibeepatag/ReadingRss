<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>ReadingRss</display-name>
  <welcome-file-list>
    <welcome-file>newsPicker.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <description></description>
    <display-name>FetchRssServlet</display-name>
    <servlet-name>FetchRssServlet</servlet-name>
    <servlet-class>edu.cmu.servlets.FetchRssServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>FetchRssServlet</servlet-name>
    <url-pattern>/FetchRssServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>FetchNews</display-name>
    <servlet-name>FetchNews</servlet-name>
    <servlet-class>edu.cmu.servlets.FetchNews</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>FetchNews</servlet-name>
    <url-pattern>/FetchNews</url-pattern>
  </servlet-mapping>
</web-app>